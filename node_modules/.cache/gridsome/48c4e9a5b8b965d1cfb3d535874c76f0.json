                 <div class=\"flex items-center md:justify-center\">\n                    <span class=\"mr-3\">Ordenar por</span>\n                    <div class=\"relative mr-8\">\n                        <button @click=\"isOpen = !isOpen\" class=\"bg-white rounded border appearance-none border-black py-2 focus:outline-none focus:border-orange-500 text-base pl-3 pr-10\">\n                          <span>Recien añadidas</span>\n                        </button>\n                        <div v-if=\"isOpen\" class=\"rounded shadow-md mt-2 absolute bg-black text-white  pin-t pin-l min-w-full\">\n                          <g-link to=\"/ranking/\" class=\"block px-2 py-1 hover:text-black hover:bg-orange-500 \">Calificación</g-link>      \n                          <g-link to=\"/alfabetico/\" class=\"block px-2 py-1 hover:text-black hover:bg-orange-500 \">A-Z</g-link>      \n                          <g-link to=\"/recien/\" class=\"mt-1 block px-2 py-1 hover:text-black hover:bg-orange-500 \">Recien añadida</g-link>\n                        </div>\n                        <span  class=\"absolute right-0 top-0 h-full w-10 text-center text-black  pointer-events-none flex items-center justify-center\">\n                          <svg fill=\"none\" stroke=\"currentColor\" stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" class=\"w-4 h-4\" viewBox=\"0 0 24 24\">\n                            <path d=\"M6 9l6 6 6-6\"></path>\n                          </svg>\n                        </span>\n                    </div>\n                  </div>\n    <!-- Tarjetas -->\n\n      <div class=\"flex justify-around flex-wrap \">\n        <div v-for=\"edge in $page.allMovie.edges\"\n          :key=\"edge.node.id\"\n          class=\"\n                mt-4 mb-4\n                ml-2 mr-2 \n                lg:w-1/5\n                md:w-1/2\n                w-full\">\n\n         <div>\n           <g-image :src=\"edge.node.featuredImage\"  class=\"rounded-t-md size object-cover object-center\" />\n         </div> \n          <div \n          class=\"bg-white \n                 border\n                 border-gray-600\n                 pb-4\n                 rounded-b-md\">\n          <p class=\"text-s pl-3 text-black font-thin mt-2 mb-1 \">Calificación {{edge.node.ranking}} / 10</p>\n          <h3 class=\"text-black\n                    text-s \n                    font-medium\n                    tracking-widest \n                    mt-2 mb-1 \n                    pl-3\">\n            {{edge.node.title}}\n          </h3>\n          <div>\n          <g-link v-for=\"tag in edge.node.tags\" :to=\"tag.path\" :key=\"tag.id\">  \n            <button \n                 class=\"mt-2\n                           text-xs\n                         text-black\n                         bg-orange-400\n                           border-0\n                           py-2 px-6\n                           ml-3\n                           focus:outline-none\n                           rounded\n                           hover:bg-white\">          \n            {{tag.id}}\n            </button>\n          </g-link>\n          </div>\n         <g-link :to=\"edge.node.path\"> \n             <button class=\"mt-3 \n                        text-white \n                        bg-black\n                        border-0 \n                        py-2 px-6 \n                        ml-3\n                        focus:outline-none \n                        hover:bg-orange-400 \n                        rounded\">\n          Más información\n            </button> \n          </g-link> \n          </div>\n        </div>\n      </div>\n      <div class=\"mt-10\">\n        <Pager :info=\"$page.allMovie.pageInfo\" linkClass=\"pager\" class=\"text-center\"/>\n      </div>\n    </section>\n    </layout>\n</template>\n\n<page-query>\nquery ($page: Int){\n  allMovie (sortBy: \"view\" , order: DESC, perPage:8, page: $page) @paginate{\n    pageInfo{\n          totalPages\n          currentPage \n        }\n    edges{\n      node{\n        id\n        view\n        code\n        title\n        ranking\n        featuredImage\n        path\n        tags{\n          id\n          path\n        }\n      }\n    }\n  }\n}\n</page-query>\n\n<script>\nimport { Pager} from 'gridsome'\n\n\nexport default {\n  data(){\n    return{\n      isOpen:false\n    }\n  },\n  components: {Pager}\n}\n</script>\n\n<style  scoped>\n\n.size{\n    height: 300px;\n    width: 369px;\n}\n\n.pager{\n  letter-spacing: 2px;\n  padding: 10px;\n  margin-right: 8px;\n  background-color: black;\n  color: #fff;\n}\n\n.pager:hover{\n  background: white;\n  color: black;\n}\n</style>"]}]}