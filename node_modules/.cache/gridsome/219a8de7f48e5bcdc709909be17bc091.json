ounded\n                           hover:bg-white\">          \n            {{tag.id}}\n            </button>\n          </g-link>\n          </div>\n         <g-link :to=\"edge.node.path\"> \n             <button class=\"mt-3 \n                        text-white \n                        bg-black\n                        border-0 \n                        py-2 px-6 \n                        ml-3\n                        focus:outline-none \n                        hover:bg-orange-400 \n                        rounded\">\n          Más información\n            </button> \n          </g-link> \n          </div>\n        </div>\n      </div>\n      <div class=\"mt-10\">\n        <Pager :info=\"$page.allM2021.pageInfo\" linkClass=\"pager\" class=\"text-center\"/>\n      </div>\n    </section>\n    </layout>\n</template>\n\n<page-query>\nquery ($page: Int){\n  allM2021 (sortBy: \"title\" , order: ASC, perPage:8, page: $page) @paginate{\n    pageInfo{\n          totalPages\n          currentPage \n        }\n    edges{\n      node{\n        id\n        view\n        code\n        title\n        ranking\n        featuredImage\n        path\n        tags{\n          id\n          path\n        }\n      }\n    }\n  }\n  \n}\n</page-query>\n\n<script>\nimport { Pager} from 'gridsome'\nimport Clasificacion from '~/components/Clasificacion.vue'\n\n\nexport default {\n  data(){\n    return{\n      isOpen:false\n    }\n  },\n  components: {Pager,\n  Clasificacion\n  }\n}\n</script>\n\n<style  scoped>\n\n.size{\n    height: 300px;\n    width: 369px;\n}\n\n.pager{\n  letter-spacing: 2px;\n  padding: 10px;\n  margin-right: 8px;\n  background-color: black;\n  color: #fff;\n}\n\n.pager:hover{\n  background: white;\n  color: black;\n}\n\n.estrella{\n  height: 20px;\n  width: 12px;\n}\n\n</style>"],"sourceRoot":"src/pages"}]}